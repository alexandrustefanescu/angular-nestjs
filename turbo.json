{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "format": {},
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "start": {
      "cache": false,
      "persistent": true
    },
    "start:debug": {
      "cache": false,
      "persistent": true
    },
    "start:prod": {
      "cache": false
    },
    "test": {},
    "test:watch": {
      "cache": false,
      "persistent": true
    },
    "test:cov": {},
    "test:debug": {
      "cache": false,
      "persistent": true
    },
    "test:e2e": {},
    "db:migration:run": {
      "cache": false
    },
    "db:migration:revert": {
      "cache": false
    },
    "db:migration:show": {
      "cache": false
    },
    "db:schema:sync": {
      "cache": false
    },
    "db:schema:drop": {
      "cache": false
    },
    "db:reset": {
      "cache": false
    },
    "db:query": {
      "cache": false
    },
    "db:seed": {
      "cache": false
    }
  }
}
