{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "format": {
      "cache": false
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "install": {
      "cache": false,
      "persistent": true
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "interruptible": true
    },
    "start": {
      "cache": false,
      "persistent": true,
      "interruptible": true
    },
    "start:debug": {
      "cache": false,
      "persistent": true,
      "interruptible": true
    },
    "start:prod": {
      "cache": false
    },
    "test": {
      "cache": true
    },
    "test:watch": {
      "cache": false,
      "persistent": true,
      "interruptible": true
    },
    "test:cov": {
      "cache": true,
      "outputs": ["coverage/**"]
    },
    "test:debug": {
      "cache": false,
      "persistent": true,
      "interruptible": true
    },
    "test:e2e": {
      "cache": false
    },
    "test-browser": {
      "cache": false
    },
    "db:migration:run": {
      "cache": false
    },
    "db:migration:revert": {
      "cache": false
    },
    "db:migration:show": {
      "cache": false
    },
    "db:schema:sync": {
      "cache": false
    },
    "db:schema:drop": {
      "cache": false
    },
    "db:reset": {
      "cache": false
    },
    "db:query": {
      "cache": false
    },
    "db:seed": {
      "cache": false
    }
  }
}
